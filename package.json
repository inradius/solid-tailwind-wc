{
  "name": "@inradius/solid-tailwind-wc",
  "version": "0.0.0",
  "description": "A Solid.js with Tailwind web component scaffold.",
  "type": "module",
  "scripts": {
    "build": "pnpm build:app:solid && pnpm build:app:wc",
    "build:app:solid": "vite build builds/build-app/app",
    "build:app:wc": "vite build builds/build-app/wc",
    "dev": "vite builds/build-app/app --host --open",
    "lint": "eslint src",
    "prettier": "prettier src -c",
    "prettier:fix": "prettier src -w",
    "preview": "vite preview --outDir dist/qa --open",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@solid-primitives/destructure": "^0.2.2",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "^4.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "4.0.18",
    "component-register": "^0.8.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-solid": "^0.14.5",
    "filemanager-plugin": "^2.9.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "prettier": "3.8.1",
    "rollup": "^4.55.3",
    "solid-devtools": "^0.34.5",
    "solid-element": "^1.9.1",
    "solid-js": "^1.9.11",
    "tailwindcss": "^4.2.1",
    "terser": "^5.46.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-plugin-solid": "2.11.10",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "msw": {
    "workerDirectory": [
      "src/builds/build-app/public"
    ]
  }
}
